{
  "name": "copland",
  "version": "2.0.0",
  "main": "js/main.js",
  "scripts": {
    "build": "mkdir -p build && browserify js/main.js -o build/build.js",
    "watch": "mkdir -p build && watchify js/main.js -o build/build.js -dv",
    "serve": "open http://localhost:9876 && python -m SimpleHTTPServer 9876",
    "lint": "eslint ."
  },
  "author": "Taylor Baldwin <taylorbaldwin@gmail.com> (https://tbaldw.in)",
  "dependencies": {
    "encode-object": "^0.1.1",
    "howler": "^1.1.28",
    "keymirror": "^0.1.1",
    "left-pad": "^1.1.1",
    "react": "^0.14.1",
    "react-dom": "^0.14.1",
    "react-redux": "^4.0.0",
    "redux": "^3.0.4",
    "redux-logger": "^2.6.1",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.3",
    "babel-plugin-transform-object-rest-spread": "^6.1.18",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "eslint": "^1.7.3",
    "eslint-plugin-react": "^3.6.3",
    "markdownify": "^0.1.0",
    "watchify": "^3.6.1"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "react",
            "es2015"
          ],
          "plugins": [
            "transform-object-rest-spread"
          ]
        }
      ],
      "markdownify"
    ]
  },
  "jest": {
    "testDirectoryName": "tests",
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest"
  }
}
